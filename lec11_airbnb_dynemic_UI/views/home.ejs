<%- include('partials/head') %>
  </head>
  <body>
    <%- include('partials/nav') %>
    <main class="container mx-auto bg-gradient-to-r from-purple-400 via-pink-500 to-red-500 shadow-2xl rounded-3xl p-10 mt-12 max-w-2xl transform transition duration-500 hover:scale-105">
      <h1>Here are our registered homes</h1>
      <ul class="space-y-6">
      <% registeredHomes.forEach(home => { %>
        <li class="bg-white bg-opacity-75 p-6 rounded-2xl shadow-xl hover:shadow-2xl transition duration-300 transform hover:scale-105">
        <span class="text-xl text-gray-900 font-semibold"><%= home.houseName %></span>
        </li>
      <% }) %>
      </ul>
    </main>
  </body>
</html> 